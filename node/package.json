{
  "name": "alterlab",
  "version": "2.0.0",
  "description": "Official Node.js SDK for the AlterLab Web Scraping API - Extract data from any website with intelligent anti-bot bypass",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "web-scraping",
    "scraping",
    "scraper",
    "api",
    "data-extraction",
    "anti-bot",
    "javascript-rendering",
    "headless-browser",
    "proxy",
    "alterlab",
    "crawling",
    "crawler"
  ],
  "author": "AlterLab <dev@alterlab.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RapierCraft/AlterLab-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/RapierCraft/AlterLab-SDK/issues"
  },
  "homepage": "https://alterlab.io",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  }
}
